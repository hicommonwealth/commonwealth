{
  "private": true,
  "scripts": {
    "build": "chmod u+x scripts/build.sh && ./scripts/build.sh",
    "build-ci": "yarn global add node-gyp && yarn --ignore-engines && yarn build && yarn workspace commonwealth migrate-db",
    "start": "concurrently -n watcher,server -c red,green 'tsc -b -w packages/commonwealth' 'yarn workspace commonwealth start'",
    "clean": "find . -name node_modules -exec rm -rf {} \\;; yarn --ignore-engines",
    "heroku-prebuild": "yarn global add node-gyp",
    "heroku-postbuild": "chmod u+x scripts/heroku-build.sh && ./scripts/heroku-build.sh",
    "psql": "yarn --cwd packages/commonwealth psql",
    "test-api": "yarn --cwd packages/commonwealth test-api",
    "dump-db": "yarn --cwd packages/commonwealth dump-db",
    "reset-db": "yarn --cwd packages/commonwealth reset-db",
    "load-db": "yarn --cwd packages/commonwealth load-db",
    "migrate-db": "yarn --cwd packages/commonwealth migrate-db",
    "start-all": "chmod u+x scripts/start.sh && ./scripts/start.sh all",
    "start-apps": "chmod u+x scripts/start.sh && ./scripts/start.sh apps",
    "start-rmq": "chmod u+x scripts/start-docker-containers.sh && ./scripts/start-docker-containers.sh rmq",
    "start-redis": "chmod u+x scripts/start-docker-containers.sh && ./scripts/start-docker-containers.sh redis",
    "format-check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "./scripts/lint-new-work.sh",
    "lint-all": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint './packages/**/*.{ts,tsx}'",
    "lint-branch": "./scripts/lint-branch.sh",
    "lint-branch-warnings": "FAIL_WARNINGS=1 ./scripts/lint-branch.sh",
    "stylelint": "stylelint packages/commonwealth/client/styles/*",
    "chromatic": "chromatic --exit-zero-on-changes",
    "prepare": "husky install"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/commonwealth.git"
  },
  "keywords": [],
  "workspaces": [
    "packages/commonwealth",
    "packages/discord-bot",
    "packages/snapshot-listener",
    "libs/*"
  ],
  "dependencies": {
    "@capacitor/android": "^4.3.0",
    "@capacitor/cli": "^4.3.0",
    "@capacitor/core": "^4.3.0",
    "@capacitor/ios": "^4.3.0",
    "@capacitor/splash-screen": "^4.1.0",
    "@cosmjs/stargate": "^0.31.0",
    "@cosmjs/tendermint-rpc": "^0.31.0",
    "@solana/web3.js": "^1.30.2",
    "@types/moment": "^2.13.0",
    "axios": "^1.3.4",
    "dotenv": "^8.2.0",
    "ethers": "5.7.2",
    "moment": "^2.23.0",
    "sequelize": "^6.28.2",
    "typescript": "^5.0.0",
    "web3": "1.8.2",
    "web3-core": "1.8.2",
    "web3-utils": "1.8.2",
    "pg": "^8.8.0",
    "typescript-logging": "^0.6.4"
  },
  "devDependencies": {
    "@osmonauts/lcd": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^7.6.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  },
  "resolutions": {
    "quill": "^1.3.6",
    "js-sha3": "^0.8.0",
    "resolve": "^1.20.0"
  }
}
