{
  "private": true,
  "scripts": {
    "start": "yarn --cwd packages/commonwealth start",
    "build": "yarn --cwd packages/commonwealth heroku-postbuild && yarn --cwd packages/chain-events build-services",
    "psql": "yarn --cwd packages/commonwealth psql",
    "test-api": "yarn --cwd packages/commonwealth test-api",
    "dump-db": "yarn --cwd packages/commonwealth dump-db",
    "reset-db": "yarn --cwd packages/commonwealth reset-db",
    "load-db": "yarn --cwd packages/commonwealth load-db",
    "migrate-db": "yarn --cwd packages/commonwealth migrate-db",
    "start-all": "concurrently -p '{name}' -c red,green,yellow,blue,magenta -n cw-app,cw-consumer,ce-app,ce-consumer,ce-subscriber 'yarn --cwd packages/commonwealth start' 'yarn --cwd packages/commonwealth start-consumer' 'yarn --cwd packages/chain-events start-app' 'yarn --cwd packages/chain-events start-consumer' 'yarn --cwd packages/chain-events start-subscriber'"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/commonwealth.git"
  },
  "keywords": [],
  "workspaces": [
    "packages/common-common",
    "packages/chain-events",
    "packages/token-balance-cache",
    "packages/commonwealth"
  ],
  "dependencies": {
    "@cosmjs/stargate": "^0.26.1",
    "@cosmjs/tendermint-rpc": "^0.26.1",
    "@solana/web3.js": "^1.30.2",
    "@types/moment": "^2.13.0",
    "axios": "^0.21.2",
    "ethers": "5.4.3",
    "moment": "^2.23.0",
    "pg": "^8.7.1",
    "typescript": "4.3.4",
    "web3": "1.5.2",
    "web3-utils": "1.5.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
