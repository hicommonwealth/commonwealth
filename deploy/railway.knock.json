{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.railway"
  },
  "deploy": {
    "preDeployCommand": "node --import=extensionless/register --enable-source-maps ./build/server/scripts/releasePhaseEnvCheck.js",
    "startCommand": "node --import=extensionless/register --enable-source-maps ./build/server/workers/knock/knockWorker.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3,
    "region": "us-east4-eqdc4a",
    "limitOverride": {
      "containers": {
        "cpu": 1,
        "memoryBytes": 100000000
      }
    }
  }
}
