{
  "vhosts": {
    "/": {
      "connection": {
        "protocol": "amqp",
        "hostname": "localhost",
        "user": "admin",
        "password": "password",
        "port": 5672,
        "vhost": "/"
      },
      "exchanges": {
        "eventsExchange": {
          "assert": true
        }
      },
      "queues": {
        "eventsQueue": {
          "assert": true,
          "purge": true
        }
      },
      "bindings": {
        "eventsBinding": {
          "source": "eventsExchange",
          "destination": "eventsQueue",
          "destinationType": "queue",
          "bindingKey": "eQueue"
        }
      },
      "publications": {
        "eventsPub": {
          "vhost": "/",
          "queue": "eventsQueue",
          "confirm": true,
          "timeout": 10000
        }
      },
      "subscriptions": {
        "eventsSub": {
          "vhost": "/",
          "queue":"eventsQueue",
          "contentType": "application/json"
        }
      }
    }
  }
}