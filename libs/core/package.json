{
  "name": "@hicommonwealth/core",
  "private": "true",
  "type": "module",
  "version": "0.1.0",
  "description": "Core Abstractions and Shared Utilities",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "devmode": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.build.json",
    "clean": "rm -rf build && find . -type f -name '*.tsbuildinfo' -exec rm {} +",
    "check-types": "tsc --noEmit",
    "test": "NODE_OPTIONS='--import tsx/esm' NODE_ENV=test mocha 'test/**/*.spec.ts'"
  },
  "dependencies": {
    "@hicommonwealth/schemas": "workspace:*",
    "@hicommonwealth/shared": "workspace:*",
    "dotenv": "^16.0.3",
    "moment": "^2.23.0",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-http-print": "^3.1.0",
    "pino-pretty": "^10.3.1",
    "rollbar": "^2.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/sinon": "^17.0.3",
    "sinon": "^17.0.2",
    "tsx": "^4.7.2"
  }
}
