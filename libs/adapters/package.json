{
  "name": "@hicommonwealth/adapters",
  "private": "true",
  "type": "module",
  "version": "0.1.0",
  "description": "External adapters",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "devmode": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build && find . -type f -name '*.tsbuildinfo' -exec rm {} +",
    "check-types": "tsc --noEmit",
    "test": "NODE_OPTIONS='--import tsx/esm' NODE_ENV=test mocha 'test/**/*.spec.ts'"
  },
  "dependencies": {
    "@hicommonwealth/core": "workspace:*",
    "@hicommonwealth/logging": "workspace:*",
    "@hicommonwealth/shared": "workspace:*",
