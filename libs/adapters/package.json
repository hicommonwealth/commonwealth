{
  "name": "@hicommonwealth/adapters",
  "private": "true",
  "type": "module",
  "version": "0.1.0",
  "description": "External adapters",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "devmode": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build && rm -rf coverage && find . -type f -name '*.tsbuildinfo' -exec rm {} +",
    "check-types": "tsc --noEmit",
    "test": "NODE_ENV=test vitest --config ../../vite.config.ts --coverage run test",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint -c ../../.eslintrc.cjs './src/**/*.{ts,tsx}'",
    "lint-diff": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint -c ../../.eslintrc-diff.cjs './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/lib-storage": "^3.578.0",
    "@aws-sdk/s3-request-presigner": "^3.577.0",
    "@hicommonwealth/core": "workspace:*",
    "@hicommonwealth/shared": "workspace:*",
    "@knocklabs/node": "^0.6.13",
    "@trpc/server": "^10.45.1",
    "amqplib": "^0.10.3",
    "async-rwlock": "^1.1.1",
    "express": "^4.20.0",
    "hot-shots": "^9",
    "lodash": "^4.17.21",
    "mixpanel": "^0.14.0",
    "moment": "^2.23.0",
    "node-fetch": "2",
    "openapi-types": "=12.1.3",
    "passport": "^0.7.0",
    "pg": "^8.11.3",
    "rascal": "^14.4.5",
    "redis": "4.2.0",
    "rollbar": "^2.6.1",
    "sequelize": "^6.32.1",
    "swagger-ui-express": "^5.0.0",
    "trpc-swagger": "^1.2.6",
    "typescript-logging": "^0.6.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.0",
    "@types/qs": "^6.9.14",
    "@types/sinon": "^17.0.3",
    "@types/superagent": "4.1.13",
    "@types/swagger-ui-express": "^4.1.6",
    "sinon": "^17.0.2",
    "superagent": "^5.2.2",
    "tsx": "^4.7.2"
  }
}
