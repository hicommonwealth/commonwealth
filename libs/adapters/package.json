{
  "name": "@hicommonwealth/adapters",
  "private": "true",
  "type": "module",
  "version": "0.1.0",
  "description": "External adapters",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "devmode": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build && rm -rf coverage && find . -type f -name '*.tsbuildinfo' -exec rm {} +",
    "check-types": "tsc --noEmit",
    "test": "NODE_ENV=test vitest --config ../../vite.config.ts --coverage run test",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint -c ../../.eslintrc.cjs './src/**/*.{ts,tsx}'",
    "lint-diff": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint -c ../../.eslintrc-diff.cjs './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/lib-storage": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.693.0",
    "@hicommonwealth/core": "workspace:*",
    "@hicommonwealth/shared": "workspace:*",
    "@knocklabs/node": "^0.6.15",
    "@trpc/server": "^10.45.2",
    "amqplib": "^0.10.4",
    "async-rwlock": "^1.1.1",
    "express": "^4.21.1",
    "hot-shots": "^9.3.0",
    "lodash": "^4.17.21",
    "mixpanel": "^0.14.0",
    "moment": "^2.30.1",
    "node-fetch": "^2.7.0",
    "openapi-types": "^12.1.3",
    "passport": "^0.7.0",
    "pg": "^8.13.1",
    "rascal": "^14.4.5",
    "redis": "4.2.0",
    "rollbar": "^2.26.4",
    "sequelize": "^6.37.5",
    "swagger-ui-express": "^5.0.1",
    "trpc-swagger": "^1.2.6",
    "typescript-logging": "^0.6.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.6",
    "@types/qs": "^6.9.17",
    "@types/sinon": "^17.0.3",
    "@types/superagent": "4.1.13",
    "@types/swagger-ui-express": "^4.1.7",
    "sinon": "^17.0.2",
    "superagent": "^5.3.1",
    "tsx": "^4.19.2"
  }
}
