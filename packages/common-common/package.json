{
  "name": "common-common",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/commonwealth.git"
  },
  "keywords": [],
  "scripts": {
