    "pg": "^8.8.0",
    "rascal": "^14.4.5",
    "ts-generator": "^0.1.1",
    "ts-mocha": "^10.0.0",
    "typechain": "^4.0.1",
    "typescript-logging": "^0.6.4"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
