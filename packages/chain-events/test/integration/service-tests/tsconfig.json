{
    "compilerOptions": {
        "allowJs": true,
        "target": "es6",
        "lib": [
            "es2019"
        ],
        "moduleResolution": "node",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "outDir": "./build",
        "baseUrl": ".",
        "declaration": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "sourceMap": true,
        "typeRoots": [
            "../../../node_modules/@types",
            "../../../../../node_modules/@types"
        ]
    },
    "paths": {
        "common-common": ["../../../../packages/common-common"],
        "common-common/*": ["../../../../packages/common-common/*"]
    },
    "include": [
        "chainEventsConsumer.test.ts"
    ]
}
