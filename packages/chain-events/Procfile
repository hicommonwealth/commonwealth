subscriber2: CHAIN_SUBSCRIBER_INDEX=1 node --max_old_space_size=$(../../scripts/get-max-old-space-size.sh) packages/chain-events/build/chain-events/services/ChainSubscriber/chainSubscriber.js run-as-script
