{
  "name": "chain-events",
  "version": "0.13.3",
  "description": "Listen to various chains for events.",
  "license": "GPL-3.0",
  "files": [
    "src/"
  ],
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist/ && tsc --project tsconfig.src.json && copyfiles -u 1 \"./src/**/*.d.ts\" \"dist\"",
    "build-services": "tsc --project tsconfig.services.json && tsc-alias --project tsconfig.services.json",
    "compile-contracts": "cd eth && npx hardhat compile && npx eslint --no-ignore --no-inline-config --fix \"./src/contractTypes/**/*.ts\"",
    "check-services-types": "tsc --noEmit --project tsconfig.services.json",
    "check-src-types": "tsc --noEmit --project tsconfig.src.json",
    "check-types": "tsc --noEmit --project tsconfig.json",
    "test-query": "ts-node ./scripts/testSequelizeQuery.ts",
    "unit-test": "ts-mocha --config ./.mocharc.json ./test/unit/**/*.spec.ts",
    "integration-test": "cd eth && hardhat test ../test/integration/*.spec.ts",
    "test": "ts-mocha --bail --project test/integration/service-tests/tsconfig.json test/integration/service-tests/EVM/aave.spec.ts --timeout 80000",
    "test-services": "ts-mocha --bail --project test/integration/service-tests/tsconfig.json test/integration/service-tests/**/*.spec.ts --timeout 240000",
    "test-app": "ts-mocha --project test/integration/service-tests/tsconfig.json test/integration/service-tests/chainEventsApp.test.ts",
    "lint-all": "eslint './**/*.ts'",
    "listen": "ts-node -T ./scripts/listener.ts",
    "listenV2": "ts-node -T ./scripts/listenerV2.ts",
    "listen-archival": "ts-node -T ./scripts/listener.ts -n edgeware-local -a true",
    "scrape": "ts-node -T ./scripts/scraper.ts",
    "ganache": "ganache-cli -m \"Alice\" -p 9545 -l 800000000 --allowUnlimitedContractSize",
    "batch-poll": "ts-node -T ./scripts/batchPoller.ts",
    "preyalcpublish": "yarn build",
    "emit-events": "ts-node --project tsconfig.json ./scripts/emitChainEvents.ts",
    "start-app": "ts-node-dev --max-old-space-size=4096 --respawn --transpile-only --project tsconfig.services.json services/app/Server.ts",
    "start-consumer": "ts-node-dev --max-old-space-size=4096 --respawn --transpile-only --project tsconfig.services.json services/ChainEventsConsumer/chainEventsConsumer.ts",
    "start-subscriber": "ts-node-dev --max-old-space-size=4096 --respawn --transpile-only --project tsconfig.services.json services/ChainSubscriber/chainSubscriber.ts",
    "start-all": "concurrently -p '{name}' -c yellow,blue,magenta -n app,consumer,subscriber 'yarn start-app' 'yarn start-consumer' 'yarn start-subscriber'",
    "create-db": "npx sequelize db:create",
    "migrate-db": "npx sequelize db:migrate",
    "create-migration": "npx sequelize migration:generate --name",
    "psql": "chmod u+x scripts/start-psql.sh && ./scripts/start-psql.sh",
    "reset-db": "chmod u+x scripts/reset-db.sh && ./scripts/reset-db.sh",
    "load-db": "chmod u+x scripts/load-db.sh && ./scripts/load-db.sh",
    "dump-db": "pg_dump $(heroku config:get DATABASE_URL -a chain-events) --verbose --no-privileges --no-owner -f latest.dump",
    "dump-db-empty": "pg_dump $(heroku config:get DATABASE_URL -a chain-events) --exclude-table-data=\"public.\\\"ChainEntities\\\"\" --exclude-table-data=\"public.\\\"ChainEventTypes\\\"\" --exclude-table-data=\"public.\\\"ChainEvents\\\"\" --verbose --no-privileges --no-owner -f latest.dump",
    "migrate-events": "NODE_OPTIONS=--max-old-space-size=$(../../scripts/get-max-old-space-size.sh) ts-node --project tsconfig.json scripts/migrateChainEntities.ts",
    "cosmos:build": "chmod u+x cosmos-chain-testing/v1/start.sh && ./cosmos-chain-testing/v1/start.sh --build && chmod u+x cosmos-chain-testing/v1beta1/start.sh && ./cosmos-chain-testing/v1beta1/start.sh --build",
    "cosmos:start": "chmod u+x cosmos-chain-testing/v1/start.sh && ./cosmos-chain-testing/v1/start.sh && chmod u+x cosmos-chain-testing/v1beta1/start.sh && ./cosmos-chain-testing/v1beta1/start.sh",
    "cosmos:stop": "chmod u+x cosmos-chain-testing/v1/stop.sh && ./cosmos-chain-testing/v1/stop.sh && chmod u+x cosmos-chain-testing/v1beta1/stop.sh && ./cosmos-chain-testing/v1beta1/stop.sh"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/chain-events.git"
  },
  "bugs": {
    "url": "https://github.com/hicommonwealth/chain-events/issues"
  },
  "homepage": "https://github.com/hicommonwealth/chain-events#readme",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@osmonauts/lcd": "^0.11.1",
    "@polkadot/api": "6.0.5",
    "@polkadot/api-derive": "6.0.5",
    "@polkadot/types": "6.0.5",
    "@polkadot/util": "7.4.1",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "bn.js": "^5.1.3",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "ethereum-block-by-date": "^1.4.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "rollbar": "^2.25.2",
    "sleep-promise": "^8.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript-logging": "^0.6.4",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@aave/aave-token": "^1.0.4",
    "@babel/core": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^2.4.0",
    "@openzeppelin/contracts-governance": "npm:@openzeppelin/contracts@^4.3.2",
    "@typechain/ethers-v5": "^6.0.0",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^10.0.0",
    "@types/node": "^14.0.14",
    "@types/underscore": "^1.10.1",
    "@types/yargs": "^15.0.9",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "ganache-cli": "^6.9.1",
    "hardhat": "^2.6.4",
    "hardhat-typechain": "^0.3.5",
    "install-peers-cli": "^2.2.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "2.0.2",
    "rimraf": "^3.0.2",
    "ts-generator": "^0.1.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^8.10.2",
    "typechain": "^4.0.1",
    "typescript": "^4.3.4",
    "yargs": "^16.1.0"
  }
}
