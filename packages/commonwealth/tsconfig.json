{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "build",
    "jsx": "react",
    "sourceMap": true,
    "strictNullChecks": true,
    "paths": {
      "@hicommonwealth/*": ["../../libs/*/src/index.ts"],
      "@hicommonwealth/model/db": ["../../libs/model/src/database.ts"],
      "@hicommonwealth/model/models": ["../../libs/model/src/models/index.ts"],
      "@hicommonwealth/model/middleware": [
        "../../libs/model/src/middleware/index.ts"
      ],
      "@hicommonwealth/model/services": [
        "../../libs/model/src/services/index.ts"
      ],
      "@hicommonwealth/model/tbc": [
        "../../libs/model/src/services/tokenBalanceCache/index.ts"
      ],
      "@hicommonwealth/model/protocol": [
        "../../libs/model/src/services/commonProtocol/index.ts"
      ],
      "@hicommonwealth/model/tester": ["../../libs/model/src/tester/index.ts"],
      "*": ["./*", "shared/*", "client/scripts/*", "client/assets/*"]
    },
    "types": ["vite/client"]
  },
  "include": [
    "client",
    "globals.d.ts",
    "server",
    "server.ts",
    "server-test.ts",
    "scripts",
    "shared",
    "test"
  ],
  "references": [
    { "path": "../../libs/adapters/tsconfig.build.json" },
    { "path": "../../libs/chains/tsconfig.build.json" },
    { "path": "../../libs/core/tsconfig.build.json" },
    { "path": "../../libs/model/tsconfig.build.json" },
    { "path": "../../libs/schemas/tsconfig.build.json" },
    { "path": "../../libs/shared/tsconfig.build.json" },
    { "path": "../../libs/evm-testing/tsconfig.build.json" },
    { "path": "../../libs/sitemaps/tsconfig.build.json" },
    { "path": "../../libs/evm-protocols/tsconfig.build.json" }
  ]
}
