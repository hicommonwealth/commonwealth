web: cd packages/commonwealth && NODE_OPTIONS=--max_old_space_size=460 ts-node -P tsconfig.server.json -T server.ts
consumer: cd packages/commonwealth && node --max_old_space_size=460 build/commonwealth/server/CommonwealthConsumer/CommonwealthConsumer.js run-as-script
release: cd packages/commonwealth && npx sequelize-cli db:migrate --config server/sequelize.json
